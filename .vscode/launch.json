{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "request": "launch",
      "name": "vuejs: firefox",
      "url": "http://localhost:8080",
      "breakOnLoad": true,
      "pathMappings": [
        {
          "url": "webpack:///src",
          "path": "${workspaceFolder}/frontend/src"
        }
      ]
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "vuejs: chrome",
      "url": "http://localhost:8080",
      "breakOnLoad": true,
      "pathMappings": [
        {
          "url": "webpack:///src",
          "path": "${workspaceFolder}/frontend/src"
        }
      ]
    }
  ]
}
